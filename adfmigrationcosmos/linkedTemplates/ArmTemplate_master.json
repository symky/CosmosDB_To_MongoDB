{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adfmigrationcosmos"
		},
		"CosmosDbMongoDb1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'CosmosDbMongoDb1'"
		},
		"MongoDb1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'MongoDb1'"
		},
		"CosmosDbMongoDb1_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "databaseMigration"
		},
		"MongoDb1_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "mydb"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adfmigrationcosmos_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"CosmosDbMongoDb1_connectionString": {
						"value": "[parameters('CosmosDbMongoDb1_connectionString')]"
					},
					"MongoDb1_connectionString": {
						"value": "[parameters('MongoDb1_connectionString')]"
					},
					"CosmosDbMongoDb1_properties_typeProperties_database": {
						"value": "[parameters('CosmosDbMongoDb1_properties_typeProperties_database')]"
					},
					"MongoDb1_properties_typeProperties_database": {
						"value": "[parameters('MongoDb1_properties_typeProperties_database')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}